#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/rollup/dist/bin/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/rollup/dist/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/rollup/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/node_modules:/mnt/c/Users/Utilisateur/Documents/node_modules:/mnt/c/Users/Utilisateur/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/rollup/dist/bin/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/rollup/dist/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/rollup/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/node_modules:/mnt/c/Users/Utilisateur/Documents/node_modules:/mnt/c/Users/Utilisateur/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
