else
  export NODE_PATH="/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/esbuild/bin/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/esbuild/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/node_modules:/mnt/c/Users/Utilisateur/Documents/node_modules:/mnt/c/Users/Utilisateur/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Utilisateur/Documents/1- Alexandre/Coding/BaseBatches/spark-onchain-app/node_modules/.pnpm/node_modules:$NODE_PATH"
